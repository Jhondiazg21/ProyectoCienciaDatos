    "# Creamos el pipeline que integra el preprocesador (preprocessor_median) y el clasificador\n",
    "pipeline_rf_median = Pipeline([\n",
    "    (\"preprocessing\", preprocessor_median),\n",
    "    (\"classifier\", RandomForestClassifier(random_state=42))\n",
    "])\n",
    "# Definimos el grid de hiperpar치metros a explorar\n",
    "param_grid = {\n",
    "    \"classifier__n_estimators\": [50, 100, 200],    # N칰mero de 치rboles\n",
    "    \"classifier__max_depth\": [None, 5, 10],          # Profundidad m치xima del 치rbol (None para sin l칤mite)\n",
    "    \"classifier__min_samples_split\": [2, 5, 10]      # N칰mero m칤nimo de muestras para dividir un nodo\n",
    "}\n",
    "# Configuramos GridSearchCV para evaluar con 5 folds y usando la m칠trica \"accuracy\"\n",
    "grid_search_rf = GridSearchCV(\n",
    "    pipeline_rf_median,\n",
    "    param_grid,\n",
    "    cv=5,\n",
    "    scoring=\"accuracy\",\n",
    "    n_jobs=-1  # Utiliza todos los cores disponibles\n",
    ")\n",
    "# Ejecutamos el grid search usando los datos de entrenamiento\n",
    "grid_search_rf.fit(X_train, y_train)\n",
    "\n",
    "# Mostramos los mejores par치metros y el mejor accuracy obtenido en validaci칩n cruzada\n",
    "print(\"Mejores par치metros:\", grid_search_rf.best_params_)\n",
    "print(\"Mejor accuracy (CV):\", grid_search_rf.best_score_)"
